{
  "name": "react-ts-start-v2",
  "version": "0.1.0",
  "description": "Заготовка под проект на react-redux стеке",
  "engines": {
    "node": ">=16.20.1"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "npm run jest",
    "jest": "jest --coverage",
    "start": "npm run clean && npm run addAssets && npm run develop",
    "build": "npm run tsc && npm run clean && npm run addAssets && npm run production && npm run clearScss",
    "develop": "webpack serve --config ./webpack/app/develop",
    "production": "webpack --config ./webpack/app/production",
    "addStyles": "npx mkdirp ./docs/assets && npx ncp ./node_modules/styles-esphere ./docs/assets",
    "addAssets": "npm run addStyles && npx ncp ./public ./docs",
    "clean": "npx rimraf ./docs",
    "clearScss": "npx rimraf ./docs/assets/scss",
    "tsc": "tsc --noEmit",
    "lint": "eslint src --color --ext .tsx,.ts --fix",
    "lint:report": "eslint src --ext .tsx,.ts --fix --format json -o reports/eslint-report.json",
    "check:reports": "npm run lint:report && npm run jest",
    "pre-commit": "lint-staged",
    "updateDependencies": "npm i korus-ui@latest && npm i styles-esphere@latest",
    "genChangelog": "change-gen gen",
    "templateUpdateKorusUiCreateBranch": "git checkout -b feature/update-korus-ui",
    "templateUpdateKorusUiOpenMR": "git push -o merge_request.create -o merge_request.target=master -o merge_request.remove_source_branch origin feature/update-korus-ui",
    "templateUpdateKorusUiCommitUpdate": "git add . && git commit -m 'update-korus-ui'",
    "templateUpdateKorusUI": "npm run templateUpdateKorusUiCreateBranch && npm run updateDependencies && npm run build && npm run templateUpdateKorusUiCommitUpdate && npm run templateUpdateKorusUiOpenMR"
  },
  "repository": {
    "type": "git",
    "url": "http://git.esphere.local/frontend/react-ts-start-v2"
  },
  "keywords": [
    "webpack",
    "react",
    "redux",
    "typescript",
    "redux-saga",
    "korus-ui",
    "styles-esphere"
  ],
  "author": "Aleksei Smirnov<smirnov.aa@esphere.ru>",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "1.5.0",
    "axios": "0.21.1",
    "date-fns": "2.17.0",
    "history": "4.10.1",
    "korus-ui": "0.78.0",
    "qs": "6.10.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.9",
    "react-resize-detector": "9.1.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "redux-first-history": "5.2.0",
    "redux-saga": "1.1.3",
    "styles-esphere": "2.2.354",
    "uuid": "7.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7.22.5",
    "@babel/core": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@jest/transform": "26.1.0",
    "@jest/types": "26.1.0",
    "@types/history": "4.7.9",
    "@types/jest": "26.0.5",
    "@types/node": "14.0.23",
    "@types/qs": "6.9.6",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.1.7",
    "@types/uuid": "8.0.0",
    "@types/webpack-env": "1.18.4",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "babel-loader": "9.1.2",
    "babel-plugin-module-resolver": "5.0.0",
    "change-gen": "1.0.6",
    "css-loader": "6.10.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-korus": "0.1.3",
    "eslint-config-korus-base": "0.1.3",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsdoc": "28.5.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-optimize-regex": "1.2.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "favicons": "7.0.0",
    "favicons-webpack-plugin": "6.0.1",
    "html-webpack-plugin": "5.5.3",
    "husky": "8.0.0",
    "jest": "26.6.3",
    "jest-cli": "26.1.0",
    "lint-staged": "10.4.0",
    "mini-css-extract-plugin": "2.8.1",
    "prettier": "2.0.5",
    "prettier-config-korus": "0.1.1",
    "ts-jest": "26.5.4",
    "ts-loader": "8.0.1",
    "typescript": "4.3.5",
    "webpack": "5.87.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "5.9.0"
  }
}
